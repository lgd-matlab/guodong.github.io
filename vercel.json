{
  "version": 2,
  "name": "acdmatic-opt",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "_site"
      }
    }
  ],
  "buildCommand": "bundle exec jekyll build",
  "installCommand": "bundle install && npm install",
  "outputDirectory": "_site",
  "framework": "other",
  "functions": {},
  "regions": ["iad1"],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/files/(.*)",
      "dest": "/files/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "frameworkDetection": {
    "rules": [
      {
        "src": "_config.yml",
        "framework": "other"
      }
    ]
  },
  "build": {
    "env": {
      "NODE_VERSION": "18",
      "RUBY_VERSION": "3.2",
      "VERCEL_IGNORE_RUBY": "false",
      "JEKYLL_ENV": "production",
      "BUNDLE_WITHOUT": "development"
    },
    "cwd": "."
  }
}